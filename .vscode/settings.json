{
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.quoteStyle": "double",
  "typescript.tsdk": "./node_modules/typescript/lib",
  "editor.tabSize": 2,
  "prettier.bracketSpacing": false,
  "files.exclude": {
    ".bash_history": true,
    ".viminfo": true,
    ".vimrc": true,
    ".vscode": true,
    "**/node_modules": true
  },
  "search.exclude": {
    "**/build": true
  },
  "eslint.options": {"configFile": "config-files/.eslintrc.js"}
}
